{
  "name": "nucampsite",
  "version": "1.0.0",
  "description": "This is a webstite for the fictional campsites review site Nucamp",
  "main": "index.html",
  "scripts": {
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "author": "Bohdan Kornatskyi",
  "license": "ISC",
  "devDependencies": {
    "light-server": "^2.9.1",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1",
    "node-sass": "^5.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "lite-server": "^2.6.1",
    "popper.js": "^1.16.1"
  }
}
